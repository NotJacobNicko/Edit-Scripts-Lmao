local CoinFolder = nil

function isAcoin(sample)
	if sample and sample:IsA("BasePart") and sample.Size == Vector3.new(2,2,1) and sample:FindFirstChildWhichIsA("TouchTransmitter") then
		return true
	else
		return false
	end
end
function TouchObj(Instan)
	local R6Limbs = {"Head","Torso","HumanoidRootPart"}
	local R15Limbs = {"Head","UpperTorso","LowerTorso","HumanoidRootPart"}
	local char = game:GetService("Players").LocalPlayer.Character
	if not char then
		return
	end
	local Root = nil
	local Selected = R6Limbs[math.random(1,#R6Limbs)]
	if char:FindFirstChild(Selected) then
		Root = char:FindFirstChild(Selected)
	else
		Root = char:FindFirstChild(R15Limbs[math.random(1,#R15Limbs)]) or char:FindFirstChild("Head")
	end
	if not Root then
		warn("Root not Found")
		return
	end
	firetouchinterest(Root, Instan, 0)
	firetouchinterest(Root, Instan , 1)
end
local IsFound = true
local CollectCoins = function()
	task.spawn(function()
		if not CoinFolder then
			for i,fol in pairs(workspace:GetDescendants()) do
				if fol:IsA("Model") and fol.Name == "CoinContainer" and fol:FindFirstChild("Coin_Server") then
					if isAcoin(fol:FindFirstChild("Coin_Server") or fol:FindFirstChildWhichIsA("Part")) then
						warn("Found Folder")
						IsFound = true
						CoinFolder = fol
						break
					else
						warn("folder is made to be fake!")
					end
				end
			end
		end

		if CoinFolder then
			for i,coins in CoinFolder:GetChildren() do
				task.spawn(function()
					if coins:IsA("BasePart") and isAcoin(coins) then
						print("grabbing coins")
						TouchObj(coins)
					else
						print("false coins")
					end
				end)
			end
		else
			if IsFound then
				IsFound = false
				warn("Folder Not Found")
			end
		end
	end)
end
warn("Collecting coins lmao")



while wait() do
	CollectCoins()
end
