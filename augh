local CoinFolder = nil

function isAcoin(sample)
	if sample and sample:IsA("BasePart") and sample.Size == Vector3.new(2,2,1) and sample:FindFirstChildWhichIsA("TouchTransmitter") and sample:FindFirstChild("CoinVisual") then
		return true
	else
		return false
	end
end
function TouchObj(Instan)
	local R6Limbs = {"Head","Torso","HumanoidRootPart"}
	local R15Limbs = {"Head","UpperTorso","LowerTorso","HumanoidRootPart"}
	local char = game:GetService("Players").LocalPlayer.Character
	if not char then
		return
	end
	local Root = nil
	local Selected = R6Limbs[math.random(1,#R6Limbs)]
	if char:FindFirstChild(Selected) then
		Root = char:FindFirstChild(Selected)
	else
		Root = char:FindFirstChild(R15Limbs[math.random(1,#R15Limbs)]) or char:FindFirstChild("Head")
	end
	if not Root then
		warn("Root not Found")
		return
	end
	firetouchinterest(Root, Instan, 0)
	firetouchinterest(Root, Instan , 1)
end
local SelectedTable = {}
local CollectCoins = function()
	task.spawn(function()
		if CoinFolder == nil or not CoinFolder then
			for i,fol in pairs(workspace:GetDescendants()) do
				if fol:IsA("Model") and fol.Name == "CoinContainer" and #fol:GetChildren() > 2 then
					warn("found")
					CoinFolder = fol
					break
				end
			end
		end

		if CoinFolder then
			for i,touchtrans in CoinFolder:GetDescendants() do
				task.spawn(function()
					if touchtrans:IsA("TouchTransmitter") and touchtrans.Parent:IsA("BasePart") then
						if not table.find(SelectedTable,touchtrans) then
							table.insert(SelectedTable,touchtrans)
							Instance.new("Highlight",touchtrans.Parent).OutlineColor = Color3.new(1, 0.933333, 0)
						end
						local Coin = touchtrans.Parent
						if game:GetService("Players").LocalPlayer.Character then
							if game:GetService("Players").LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
								game:GetService("Players").LocalPlayer.Character:FindFirstChild("HumanoidRootPart").Anchored = true
							end
							game:GetService("Players").LocalPlayer.Character:MoveTo(Coin.Position)
						end
						TouchObj(Coin)
					end
				end)
			end
		end
	end)
end
warn("Collecting coins lmao")
local Con

workspace.DescendantAdded:Connect(function(isn) 
	if isAcoin(isn) then
		TouchObj(isn)
	end
end)
while wait() do
	CollectCoins()
end
