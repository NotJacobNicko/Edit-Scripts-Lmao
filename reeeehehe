Visual:NewToggle("find Low Ping Server", "Teleporting To Low Ping Server", function(toggled)
	local function fetchServersData(placeId)
		local url = "https://games.roblox.com/v1/games/"..placeId.."/servers/Public?sortOrder=Asc&limit=100"
		local success, response = pcall(function()
			return game:GetService("HttpService"):JSONDecode(game:HttpGet(url))
		end)

		if success and response and response.data then
			return response.data
		end

		return nil
	end
	local servers = fetchServersData(game.PlaceId)
	if not servers then
		return
	end
	local lowestPingServer = servers[1]
	for _, server in pairs(servers) do
		if server["ping"] < lowestPingServer["ping"] and server.maxPlayers > server.playing then
			lowestPingServer = server
		end
	end
	task.wait(3)
	local dataPing = game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValueString()
	if tonumber(dataPing:match("(%d+)")) >= 100 then
		game:GetService("TeleportService"):TeleportToPlaceInstance(game.PlaceId, lowestPingServer.id)
	end
end)
