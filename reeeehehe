		Variables['Instance13'].Parent = Variables['Instance4']
		for i,v in pairs(Variables['Instance3']:GetDescendants()) do
			if v:IsA("ParticleEmitter") then
				table.insert(RGBtable,v)
			end
		end
		task.delay(2,function()
			Variables['Instance3']:Destroy()
		end)
		task.delay(.17,function()
			parrycurrently = false
			task.wait(.08)
			if SwordWeld1 ~= nil and not parrycurrently then
				SwordWeld1.C0 = CFrame.new(0.200000003, -0.550000012, -2, 0.99619472, 0.0871556997, 3.80969656e-09, 0, -4.37113883e-08, 1, 0.0871556997, -0.99619472, -4.35450538e-08)
				SwordWeld1.C1 = CFrame.new(0, 0, 0, 0.939692616, 0, 0.342020154, 0, 1, 0, -0.342020154, 0, 0.939692616)
			end
		end)
		task.delay(.2,function()
			for i,v in pairs(Variables['Instance3']:GetDescendants()) do
				if v:IsA("ParticleEmitter") then
					v.Enabled = false
				end
			end
		end)
	end)
end
function CheckChar(Char)
	local RootPart = Char:FindFirstChild("HumanoidRootPart")
	local HumanoidChar = Char:FindFirstChild("Humanoid")
	HumanoidChar.Died:Connect(function()
		local_player.CharacterAdded:Connect(function(char)
			local Character = nil
			repeat
				if local_player.Character and local_player.Character:FindFirstChildWhichIsA("Humanoid").Health ~= 0 then
					Character = local_player.Character or char
					CheckChar(Character)
				end
				wait()
			until Character ~= nil
		end)
	end)
	if SwordModel then
		SwordModel:Destroy()
	end
	
	if TogglSetting.CustomSword then
		AddSword(Char)
	end
end

CheckChar(local_player.Character)
function SwordCrateManual()
	game:GetService("ReplicatedStorage").Remote.RemoteFunction:InvokeServer("PromptPurchaseCrate", workspace.Spawn.Crates.NormalSwordCrate)
end
function ExplosionCrateManual()
	game:GetService("ReplicatedStorage").Remote.RemoteFunction:InvokeServer("PromptPurchaseCrate", workspace.Spawn.Crates.NormalExplosionCrate)
end
function SwordCrateAuto()
	while _G.AutoSword do
		game:GetService("ReplicatedStorage").Remote.RemoteFunction:InvokeServer("PromptPurchaseCrate", workspace.Spawn.Crates.NormalSwordCrate)
		wait(1)
	end
end
function ExplosionCrateAuto()
	while _G.AutoBoom do
		game:GetService("ReplicatedStorage").Remote.RemoteFunction:InvokeServer("PromptPurchaseCrate", workspace.Spawn.Crates.NormalExplosionCrate)
		wait(1)
	end
end
