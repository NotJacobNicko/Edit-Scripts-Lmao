		Variables['Instance13'].ZOffset = 0
		Variables['Instance13'].EmissionDirection = Enum.NormalId.Top
		Variables['Instance13'].Color = ColorSequence.new{ColorSequenceKeypoint.new(0,Color3.new(0.3921568691730499,0.13333334028720856,0.6000000238418579)),ColorSequenceKeypoint.new(1,Color3.new(0.3921568691730499,0.13333334028720856,0.6000000238418579))}
		Variables['Instance13'].Shape = Enum.ParticleEmitterShape.Box
		Variables['Instance13'].Size = NumberSequence.new{NumberSequenceKeypoint.new(0,13.49832820892334,0),NumberSequenceKeypoint.new(1,13.49832820892334,0)}
		Variables['Instance13'].Texture = 'rbxassetid://15187783966'
		Variables['Instance13'].Transparency = NumberSequence.new{NumberSequenceKeypoint.new(0,0,0),NumberSequenceKeypoint.new(0.7542282938957214,0,0),NumberSequenceKeypoint.new(1,1,0)}
		Variables['Instance13'].Brightness = 10
		Variables['Instance13'].Enabled = true
		Variables['Instance3'].Parent = rootpart.Parent
