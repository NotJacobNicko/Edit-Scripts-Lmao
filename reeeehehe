local parrycurrently = false
function parryeffect(rootpart)
	task.spawn(function()
		if SwordWeld1 then
			parrycurrently = true
			SwordWeld1.C0 = CFrame.new(0,0.5,0)
			SwordWeld1.C1 = CFrame.new(0,0,0)
		end
		if rootpart then
			local hit_Sound = Instance.new('Sound',local_player.Character:WaitForChild("HumanoidRootPart"))
			hit_Sound.SoundId = 'rbxassetid://1884209317'
			hit_Sound.Volume = 2
			hit_Sound.PlaybackSpeed = 1.8
