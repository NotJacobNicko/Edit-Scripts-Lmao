local connections = {}
function randomstring()
	local e = {}
	for i = 1,math.random(5,50) do
		table.insert(e,utf8.char(math.random(10,100)))
	end
	return table.concat(e)
end
local TogglSetting = {}
TogglSetting.aura_Enabled = false
TogglSetting.hit_sound_Enabled = false
TogglSetting.hit_effect_Enabled = false
TogglSetting.night_mode_Enabled = false
TogglSetting.trail_Enabled = false
TogglSetting.self_effect_Enabled = false
TogglSetting.AutoSpam = false
TogglSetting.TestParry = false
TogglSetting.CustomSword = false
local RGBtable = {}
task.defer(function()
	while true do
		for a = 1,360,1*6 do 
			wait()
			local RGBColor = Color3.fromHSV(a/360,1,1)
			pcall(function()
				if NeonSwordPart and Swordlight then
					Swordlight.Color = RGBColor
					NeonSwordPart.Color = RGBColor
					if SwordSlashEffect ~= nil then
						SwordSlashEffect.Color = ColorSequence.new(RGBColor)
					end
				end
				for i,Partrgb in pairs(RGBtable) do
					if Partrgb:IsA("ParticleEmitter") or Partrgb:IsA("Beam") and Partrgb ~= nil then
						Partrgb.Color = ColorSequence.new(RGBColor)
					else
						if Partrgb:IsA("BasePart") and Partrgb ~= nil then
							Partrgb.Color = RGBColor
						end
					end
				end
			end)
		end
	end
end)
local ThemePos = 0
function AddSword(Character)
	local UserSword = nil
	for i,Model in pairs(Character:GetChildren()) do
		if Model:IsA("Model") and Model.Name == "Dark Blade" then
			local SwordDetect = Model:FindFirstChildWhichIsA("MeshPart")
			if SwordDetect and SwordDetect.Name == "KatanaMesh" then
				if SwordDetect:FindFirstChildWhichIsA("Model") then
					SwordDetect:FindFirstChildWhichIsA("Model"):Destroy()
				end
				SwordDetect.Transparency = 1
				UserSword = SwordDetect
			end
		end
	end
	
	MainSword = game:GetService("InsertService"):CreateMeshPartAsync("rbxassetid://18858973664",Enum.CollisionFidelity.Default,Enum.RenderFidelity.Performance)
	NeonSwordPart = game:GetService("InsertService"):CreateMeshPartAsync("rbxassetid://18858973949",Enum.CollisionFidelity.Default,Enum.RenderFidelity.Performance)
	SwordModel = Instance.new("Model",UserSword)
	SwordModel.Name = randomstring()

	MainSword.Name = randomstring()
	MainSword.Parent = SwordModel
	MainSword.TextureID = "rbxassetid://18858973774"
	MainSword.CanCollide = false 
	MainSword.Size = Vector3.new(1.732, 8.075, 0.572)
	MainSword.Massless = true
	MainSword.CFrame = CFrame.new(0,10,0)

	NeonSwordPart.Name = randomstring()
	NeonSwordPart.Parent = SwordModel
	NeonSwordPart.Material = Enum.Material.Neon
	NeonSwordPart.Size = Vector3.new(0.219, 7.007, 0.371)
	NeonSwordPart.Color = Color3.new(0.968627, 0, 1)
	NeonSwordPart.CanCollide = false
	NeonSwordPart.Massless = true
	Swordlight = Instance.new("PointLight",NeonSwordPart)
	Swordlight.Name = randomstring()
	Swordlight.Brightness = 1
	Swordlight.Range = 15
	local SwordWeld = Instance.new("Weld",MainSword)
	SwordWeld.Name = randomstring()
	SwordWeld.C0 = CFrame.new(0,-0.5,0)
	SwordWeld.Part0 = MainSword
	SwordWeld.Part1 = NeonSwordPart
	SwordWeld1 = Instance.new("Weld",MainSword)
	SwordWeld1.Name = randomstring()
	SwordWeld1.C0 = CFrame.new(0.200000003, -0.550000012, -2.29999995, 0.99619472, 0.0871556923, 3.80969611e-09, 0, -4.37113883e-08, 1, 0.0871556923, -0.99619472, -4.35450538e-08)
	SwordWeld1.C1 = CFrame.new(0, 0, 0, 0.939692616, 0, 0.342020154, 0, 1, 0, -0.342020154, 0, 0.939692616)
	SwordWeld1.Part0 = UserSword
	SwordWeld1.Part1 = MainSword
	local Att1 = Instance.new("Attachment",MainSword)
	Att1.Name = randomstring()
	Att1.CFrame = CFrame.new(0,-1.65,0)
	local Att2 = Instance.new("Attachment",MainSword)
	Att2.Name = randomstring()
	Att2.CFrame = CFrame.new(0,4.05,0)
	SwordSlashEffect = Instance.new("Trail",MainSword)
	SwordSlashEffect.Name = randomstring()
	SwordSlashEffect.Enabled = false
	SwordSlashEffect.Brightness = 3
	SwordSlashEffect.Texture = "rbxassetid://1978704853"
	SwordSlashEffect.TextureLength = 0.625
	SwordSlashEffect.Transparency = NumberSequence.new{NumberSequenceKeypoint.new(0,0),NumberSequenceKeypoint.new(1,1)}
	SwordSlashEffect.Attachment0 = Att1
	SwordSlashEffect.Attachment1 = Att2
	SwordSlashEffect.Lifetime = 0.5
	SwordSlashEffect.MinLength = 0
	ThemeSong = Instance.new("Sound",MainSword)
	ThemeSong.Name = randomstring()
	ThemeSong.Looped = true
	ThemeSong.PlaybackSpeed = 0.85
	ThemeSong.SoundId = "rbxassetid://1842802040"
	ThemeSong.Volume = .5
	ThemeSong.TimePosition = ThemePos
	ThemeSong:Play()
